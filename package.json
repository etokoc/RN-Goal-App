{
  "name": "react-native-projects",
  "version": "1.0.0",
  "description": "Monorepo containing multiple React Native projects",
  "main": "index.js",
  "scripts": {
    "start:goal": "cd apps/GoalApp && npm start",
    "android:goal": "cd apps/GoalApp && npm run android",
    "ios:goal": "cd apps/GoalApp && npm run ios",
    "install:all": "npm install && cd apps/GoalApp && npm install",
    "clean": "rm -rf node_modules && cd apps/GoalApp && rm -rf node_modules"
  },
  "keywords": [
    "react-native",
    "monorepo",
    "mobile-apps"
  ],
  "author": "Your Name",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "shared/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  }
}
